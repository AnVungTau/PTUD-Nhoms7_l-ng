package util;

import entity.TaiKhoan;
import gui.ManHinhChinh;

public class Keycode {
	private static TaiKhoan taiKhoan;
	private static ManHinhChinh manHinhChinh;

	public static TaiKhoan getTaiKhoan() {
		return taiKhoan;
	}

	public static void setTaiKhoan(TaiKhoan taiKhoan) {
		Keycode.taiKhoan = taiKhoan;
	}
	
	public static ManHinhChinh getManHinhChinh() {
		return manHinhChinh = new ManHinhChinh();
	}
}
